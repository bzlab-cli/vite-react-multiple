{
	"name": "react-admin",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "npm-run-all proxy:init --parallel mock serve:dev",
		"serve:dev": "cross-env NODE_ENV=development dotenv -e .env.develop vite serve",
		"build:deploy": "cross-env NODE_ENV=production dotenv -e .env.deploy vite build",
		"build:release": "cross-env NODE_ENV=production dotenv -e .env.release vite build",
		"build": "cross-env NODE_ENV=production dotenv -e .env.prod vite build",
		"preview": "vite preview --port 8442",
		"lint:eslint": "eslint --cache --max-warnings 0 \"src/**/*.{js,ts,tsx}\" --fix",
		"lint:prettier": "prettier --write  \"src/**/*.{js,ts,json,tsx,css,less,scss,html,md}\"",
		"lint": "npm-run-all lint:eslint lint:prettier",
		"mock": "cd mock && ts-node-dev mock.ts",
		"proxy:init": "ts-node-dev @bzlab/bz-react-proxy init --config=build/proxy/config.json",
		"proxy": "ts-node-dev @bzlab/bz-react-proxy use --config=build/proxy/config.json"
	},
	"dependencies": {
		"@ant-design/icons": "^4.6.2",
		"@ant-design/pro-components": "^2.3.51",
		"@bz/bz-react-upload": "^0.2.9",
		"@bzlab/bz-react-core": "0.0.4",
		"@reduxjs/toolkit": "^1.9.1",
		"ahooks": "^3.7.4",
		"antd": "^5.4.7",
		"axios": "^0.27.2",
		"dayjs": "^1.10.7",
		"immer": "^9.0.15",
		"nprogress": "^0.2.0",
		"react": "^18.2.0",
		"react-activation": "^0.11.2",
		"react-dom": "^18.2.0",
		"react-redux": "^8.0.2",
		"react-router-dom": "^6.3.0",
		"redux": "^4.2.0",
		"redux-persist": "^6.0.0",
		"redux-promise": "^0.6.0",
		"redux-thunk": "^2.4.1",
		"screenfull": "^6.0.2",
		"styled-jsx": "^5.1.2"
	},
	"devDependencies": {
		"@bzlab/bz-react-proxy": "0.0.2",
		"@types/koa": "^2.11.6",
		"@types/koa-logger": "^3.1.1",
		"@types/koa-router": "^7.4.1",
		"@types/koa2-cors": "^2.0.1",
		"@types/lodash": "^4.14.167",
		"@types/mockjs": "^1.0.4",
		"@types/node": "^17.0.41",
		"@types/react": "^18.0.0",
		"@types/react-dom": "^18.0.0",
		"@types/react-router-dom": "^5.3.3",
		"@types/redux-promise": "^0.5.29",
		"@typescript-eslint/eslint-plugin": "^5.27.1",
		"@typescript-eslint/parser": "^5.27.1",
		"@vitejs/plugin-react": "^1.3.0",
		"autoprefixer": "^10.4.7",
		"chalk": "^4.1.0",
		"cross-env": "^7.0.3",
		"dotenv-cli": "^4.0.0",
		"eslint": "^8.17.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.30.0",
		"eslint-plugin-react-hooks": "^4.5.0",
		"husky": "^8.0.1",
		"koa": "^2.13.0",
		"koa-body": "^4.2.0",
		"koa-logger": "^3.2.1",
		"koa-router": "^10.0.0",
		"koa2-cors": "^2.0.6",
		"less": "^4.1.3",
		"lint-staged": "^13.0.2",
		"log4js": "^6.3.0",
		"mockjs": "^1.1.0",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.14",
		"prettier": "^2.6.2",
		"reflect-metadata": "^0.1.13",
		"rollup-plugin-visualizer": "^5.6.0",
		"runjs": "^4.4.2",
		"sass": "^1.26.5",
		"sass-loader": "^8.0.2",
		"ts-node-dev": "^2.0.0",
		"typescript": "^4.6.3",
		"vite": "^2.9.9",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-eslint": "^1.6.1",
		"vite-plugin-html": "^3.2.0",
		"vite-plugin-svg-icons": "^2.0.1"
	},
	"gitHooks": {
		"pre-commit": "lint-staged"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"git add"
		]
	}
}
